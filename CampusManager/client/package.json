{
  "name": "campus-manager",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run webpack-watch",
    "lint": "eslint --ext .jsx,.js webpack.config.* ./",
    "webpack-build-dev": "NODE_ENV=development webpack --config webpack.config.js --display-error-details",
    "webpack-build-prod": "NODE_E,NV=production webpack -p --config webpack.config.js",
    "webpack-watch": "webpack --watch --config webpack.config.js"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "css-loader": "0.23.1",
    "fbjs": "^0.8.16",
    "file-loader": "^0.9.0",
    "jquery": "^3.2.1",
    "material-ui": "^1.0.0-beta.19",
    "react": "^15.6.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^6.3.0",
    "prop-types": "latest",
    "webpack-hot-middleware": "^2.12.2"
  }
}
